<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ATAK_Project_needed_for_Closed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 ISBLANK(ATAK_Project__c),
 ISPICKVAL(Status, &apos;Accepted&apos;) 
)</errorConditionFormula>
    <errorMessage>Please reference an ATAK Project before marking this Quote as Accepted.</errorMessage>
</ValidationRule>

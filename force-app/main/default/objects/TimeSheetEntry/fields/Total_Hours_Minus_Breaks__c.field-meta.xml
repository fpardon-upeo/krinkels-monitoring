<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Hours_Minus_Breaks__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Resource_Absence__c) ,  
    Total_Hours__c , 
    (DurationInMinutes - ((Resource_Absence__r.End - Resource_Absence__r.Start) * 1440))/60)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Hours Minus Breaks</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
